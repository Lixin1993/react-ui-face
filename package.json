{
  "name": "@ctrip/falcon",
  "main": "index.js",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "lint": "next lint",
    "build:tsc": "tsc",
    "create": "node ./build/create",
    "check": "node ./build/check",
    "build:babel": "babel src --extensions .tsx,.ts,.js -d dist",
    "webpack": "webpack --config ./build/webpack.css.config.js",
    "build": "npm run check && rm -rf dist && npm run build:babel && npm run build:tsc && npm run webpack && node ./build/file.js",
    "clean": "rm -rf node_modules package-lock.json && npm install"
  },
  "dependencies": {
    "core-js": "^3.19.1",
    "next": "9.1.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@types/node": "^14.17.0",
    "@types/react": "^16.0.0",
    "@zeit/next-sass": "^1.0.1",
    "@zeit/next-typescript": "^1.1.1",
    "eslint": "^5.16.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "mini-css-extract-plugin": "^2.4.5",
    "node-sass": "^4.14.0",
    "typescript": "^3.0.0",
    "webpack": "^5.64.2",
    "webpack-cli": "^4.9.1"
  }
}
