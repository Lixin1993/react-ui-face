{
  "compilerOptions": {
    "target": "es5",
    "outDir": "./dist",
    "allowJs": false,
    "jsx": "preserve",
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "declaration": true,
    "emitDeclarationOnly": true,
    "incremental": true,
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "noEmit": false
  },
  "include": [
    "next-env.d.ts",
    "./components/**/*.ts",
    "./components/**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
